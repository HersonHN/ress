{
  "name": "ress",
  "version": "1.0.1",
  "description": "rss reader",
  "author": "Herson Salinas <hersonhn@gmail.com>",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "node scripts/create-dot-env.js && vue-cli-service build",
    "start": "node server.js",
    "local": "node run-for-local.js",
    "production": "node scripts/create-dot-env.js && node run-for-production.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cleanview": "^1.2.0",
    "express": "^4.16.3",
    "foundation-sites": "^6.4.4-rc1",
    "moment": "^2.22.2",
    "node-fetch": "^2.2.0",
    "promise-deferred": "^2.0.1",
    "rss-parser": "^3.4.3",
    "url-parse": "^1.4.3",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jquery": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
