{
  "version": 2,
  "public": true,
  "alias": [
    "news.herson.hn"
  ],
  "builds": [
    { "src": "index.js", "use": "@now/node-server" },
    { "src": "/dist/*", "use": "@now/static" },
    { "src": "/dist/*/*", "use": "@now/static" }
  ],
  "routes": [
    { "methods": ["GET"],  "dest": "/index.js", "src": "/api/feed" },
    { "methods": ["GET"],  "dest": "/index.js", "src": "/api/feed/(.*)" },
    { "methods": ["GET"],  "dest": "/index.js", "src": "/feed/(.*)" },
    { "methods": ["POST"], "dest": "/index.js", "src": "/api/clean" },

    { "src": "/(.*)", "dest": "/dist/$1" },
    { "src": "/css/(.*)", "dest": "/dist/css/$1" },
    { "src": "/img/(.*)", "dest": "/dist/img/$1" },
    { "src": "/js/(.*)", "dest": "/dist/js/$1" }
  ],
  "env": {
    "VUE_APP_GOOGLE_ANALYTICS": "@ress-google-analytics"
  }
}